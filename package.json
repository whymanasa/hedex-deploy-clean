{
    "name": "hed-ex",
    "version": "1.0.0",
    "description": "HED-EX Application",
    "type": "module",
    "scripts": {
      "start": "bun backend/index.js",
      "server": "cd backend && bun run start",
      "client": "cd client && bun run dev",
      "dev": "concurrently \"bun run server\" \"bun run client\"",
      "install-all": "bun install && cd client && bun install && cd ../backend && bun install",
      "build": "cd client && bun run build",
      "lint": "eslint .",
      "preview": "cd client && bun run preview"
    },
    "dependencies": {
      "@tailwindcss/vite": "^4.1.4",
      "axios": "^1.9.0",
      "concurrently": "^8.2.2",
      "docx": "^9.5.0",
      "html2canvas": "^1.4.1",
      "i18next": "^25.2.0",
      "i18next-browser-languagedetector": "^8.1.0",
      "i18next-http-backend": "^3.0.2",
      "install": "^0.13.0",
      "jspdf": "^3.0.1",
      "jspdf-autotable": "^5.0.2",
      "node-cache": "^5.1.2",
      "openai": "^4.96.2",
      "pdfjs-dist": "3.11.174",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "react-i18next": "^15.5.1",
      "react-icons": "^5.5.0",
      "react-markdown": "^10.1.0",
      "react-router-dom": "^7.5.3",
      "remark-gfm": "^4.0.1",
      "tailwindcss": "^4.1.4"
    },
    "devDependencies": {
      "@eslint/js": "^9.22.0",
      "@types/react": "^19.0.10",
      "@types/react-dom": "^19.0.4",
      "@vitejs/plugin-react": "^4.3.4",
      "eslint": "^9.22.0",
      "eslint-plugin-react-hooks": "^5.2.0",
      "eslint-plugin-react-refresh": "^0.4.19",
      "globals": "^16.0.0",
      "vite": "^6.3.1"
    }
  }